<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="480.0" styleClass="white" stylesheets="@Design.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.perproject.TimeController">
   <children>
      <Pane layoutY="732.0" prefHeight="70.0" prefWidth="480.0" styleClass="firstColor" stylesheets="@Design.css" />
      <Button fx:id="pauseBtn" layoutX="330.0" layoutY="644.0" mnemonicParsing="false" onAction="#pauseButton" prefHeight="45.0" prefWidth="110.0" stylesheets="@Design.css" text="Stop" textFill="WHITE">
         <styleClass>
            <String fx:value="thirdColor" />
            <String fx:value="round2" />
            <String fx:value="BtShadow" />
         </styleClass>
      </Button>
      <Button fx:id="newBtn" layoutX="56.0" layoutY="644.0" mnemonicParsing="false" onAction="#newButton" prefHeight="45.0" prefWidth="110.0" stylesheets="@Design.css" text="Start" textFill="WHITE">
         <styleClass>
            <String fx:value="thirdColor" />
            <String fx:value="BtShadow" />
            <String fx:value="round2" />
         </styleClass>
      </Button>
      <Pane layoutX="40.0" layoutY="93.0" prefHeight="400.0" prefWidth="400.0" stylesheets="@Design.css">
         <styleClass>
            <String fx:value="RoundImage" />
            <String fx:value="shadow" />
         </styleClass>
         <children>
            <Label fx:id="timeLabel" layoutX="131.0" layoutY="180.0" text="0:0:0">
               <font>
                  <Font size="64.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button fx:id="reStartBtn" layoutX="192.0" layoutY="644.0" mnemonicParsing="false" onAction="#reStartButton" prefHeight="45.0" prefWidth="110.0" stylesheets="@Design.css" text="Restart" textFill="WHITE">
         <styleClass>
            <String fx:value="thirdColor" />
            <String fx:value="round2" />
            <String fx:value="BtShadow" />
         </styleClass>
      </Button>
   </children>
</AnchorPane>
